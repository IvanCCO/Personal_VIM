[
  {
    "key": "ctrl+k",
    "command": "selectPrevSuggestion",
    "when": "editorTextFocus && vim.active && vim.use<C-k>"
  },
  {
    "key": "ctrl+j",
    "command": "selectNextSuggestion",
    "when": "editorTextFocus && vim.active && vim.use<C-k>"
  },
  {
    "key": "ctrl+k",
    "command": "workbench.action.quickOpenSelectPrevious",
    "when": "!editorFocus"
  },
  {
    "key": "ctrl+j",
    "command": "workbench.action.quickOpenSelectNext",
    "when": "!editorFocus"
  },
  {
    "key": "ctrl+z",
    "command": "-undo"
  },
  {
    "key": "ctrl+shift+z",
    "command": "-redo"
  },
  {
    "key": "ctrl+z",
    "command": "cursorLeft",
    "when": "editorTextFocus && vim.active && vim.use<C-k>"
  },
  {
    "key": "ctrl+s",
    "command": "cursorEnd",
    "when": "editorTextFocus && vim.active && vim.use<C-k>"
  },
  {
    "key": "ctrl+q",
    "command": "cursorUp",
    "when": "editorTextFocus && vim.active && vim.use<C-k>"
  },
  {
    "key": "ctrl+c",
    "command": "cursorDown",
    "when": "editorTextFocus && vim.active && vim.use<C-k>"
  },
  {
    "key": "alt+j",
    "command": "editor.action.moveLinesDownAction",
    "when": "editorTextFocus && vim.active"
  },
  {
    "key": "alt+k",
    "command": "editor.action.moveLinesUpAction",
    "when": "editorTextFocus && vim.active"
  },
  {
    "key": "ctrl+g",
    "command": "lazygit.openLazygit",
    "when": "vim.active"
  }
]
